cart.splice(index,1)\r\n        }\r\n            })\r\n            setCart([...cart])\r\n            addToCart()\r\n        }\r\n    }\r\n\r\n    \r\n    if (cart.length === 0)\r\n        return <h2 style={{textAlign:\"center\", fontsize:\"Srem\"}}>Cart Empty</h2>\r\n    return (\r\n        <div>\r\n            {\r\n                cart.map(product => (\r\n                    <div className='detail cart' key={product._id}>\r\n                        <img src={product.images.url} alt=\"\"  />\r\n\r\n                        <div className='box-detail'>\r\n                            <div className='row'>\r\n                                <h2>{product.title}</h2>\r\n\r\n                                <h6>#id: {product.product_id}</h6>\r\n                    \r\n                            </div>\r\n                            <span>₹ {product.price*product.quantity}</span>\r\n                            <p>{product.description}</p>\r\n                            <p>{product.content}</p>\r\n                            <div className='amount'>\r\n                                <button onClick={()=>decrement(product._id)}> - </button>\r\n                                <span>{product.quantity}</span>\r\n                                <button onClick={()=>increment(product._id)}> + </button>\r\n                            </div>\r\n\r\n                            <div className='delete' onClick={()=>removeProduct(product._id)}>X</div>\r\n                            \r\n                           \r\n                        </div>\r\n                    </div>\r\n\r\n            \r\n                ))\r\n            }\r\n            <div className='total'>\r\n                <h3>Total:₹ {total}</h3>\r\n                <Link to=\"#!\">Payment</Link>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Cart\r\n","C:\\Users\\Shri\\Desktop\\Ecommerce-Artisians\\client\\src\\components\\mainpages\\products\\Products.js",[],[],"C:\\Users\\Shri\\Desktop\